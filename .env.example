# Project Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to git!

# GCP Project Configuration
PROJECT_ID="your-project-id-here"
REGION="asia-south1"
ZONE="asia-south1-a"

# GCP Billing (optional - needed for budget alerts)
BILLING_ACCOUNT_ID="your-billing-account-id"

# Cluster Configuration
CLUSTER_NAME="sb-cluster"

# Database Configuration
DB_PASSWORD="your-super-secure-password-here"

# Cloudflare Configuration
CLOUDFLARE_TUNNEL_TOKEN="your-cloudflare-tunnel-token-here"

# GitHub Configuration (for reference)
GITHUB_USER="your-github-username"
GITHUB_REPO="your-repo-name"
